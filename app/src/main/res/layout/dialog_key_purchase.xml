<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fff7c3"
        android:elevation="20dp"
        android:layout_marginBottom="10dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="5dp">


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/my_keys"
                                android:id="@+id/textView27"
                                android:textStyle="bold|italic" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:id="@+id/myKeyVal"
                                android:layout_marginLeft="5dp"
                                android:textStyle="bold|italic" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:gravity="center_horizontal"
                            android:background="#f1518c">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="KEY STORE"
                                android:id="@+id/textView12"
                                android:textStyle="bold"
                                android:textColor="#ffffff" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="10dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="1."
                                android:id="@+id/textView16"
                                android:textStyle="bold|italic" />

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="20dp"
                                android:src="@drawable/key_pink"
                                android:layout_marginTop="5dp"
                                android:foregroundTint="#694f4f"
                                android:id="@+id/so" />

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:text="X 10 "
                                android:layout_marginTop="5dp"
                                android:textStyle="bold|italic" />

                            <TextView
                                android:layout_width="85dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:id="@+id/textView18"
                                android:text="KW 1000"
                                android:textStyle="bold|italic" />

                            <CheckBox
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/CB1"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:checked="false" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="10dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="2."
                                android:id="@+id/textView17"
                                android:textStyle="bold|italic" />

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:id="@+id/imageView"
                                android:src="@drawable/key_pink"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="5dp" />

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:text="X 35"
                                android:textStyle="bold|italic" />

                            <TextView
                                android:layout_width="85dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:text="KW 3000"
                                android:layout_marginTop="5dp"
                                android:id="@+id/textView19"
                                android:textStyle="bold|italic" />

                            <CheckBox
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/CB2"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:checked="false" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="10dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="3."
                                android:id="@+id/textView23"
                                android:textStyle="bold|italic" />

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:id="@+id/imageView2"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="5dp"
                                android:src="@drawable/key_pink" />

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:text="X 70"
                                android:textStyle="bold|italic" />

                            <TextView
                                android:layout_width="85dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:text="KW 6000"
                                android:layout_marginTop="5dp"
                                android:id="@+id/textView22"
                                android:textStyle="bold|italic" />

                            <CheckBox
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/CB3"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:checked="false" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="10dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="4."
                                android:id="@+id/textView24"
                                android:textStyle="bold|italic" />

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:id="@+id/imageView3"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="5dp"
                                android:src="@drawable/key_pink" />

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:text="X 250"
                                android:textStyle="bold|italic" />

                            <TextView
                                android:layout_width="85dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:text="KW 20000"
                                android:layout_marginTop="5dp"
                                android:id="@+id/textView20"
                                android:textStyle="bold|italic" />

                            <CheckBox
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/CB4"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:checked="false" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="5."
                                android:id="@+id/textView25"
                                android:textStyle="bold|italic" />

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:id="@+id/imageView4"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="5dp"
                                android:src="@drawable/key_pink" />

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:text="X 600"
                                android:textStyle="bold|italic" />

                            <TextView
                                android:layout_width="85dp"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:layout_marginLeft="5dp"
                                android:text="KW 50000"
                                android:layout_marginTop="5dp"
                                android:id="@+id/textView21"
                                android:textStyle="bold|italic" />

                            <CheckBox
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/CB5"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:checked="false" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="#f1518c"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"></TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center_horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="CANCEL"
                                android:id="@+id/cancelText"
                                android:textStyle="bold"
                                android:layout_marginRight="80dp"
                                android:layout_marginLeft="35dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="BUY"
                                android:id="@+id/buyText"
                                android:textStyle="bold"
                                android:layout_marginLeft="0dp"
                                android:layout_marginTop="0dp"
                                android:layout_marginRight="50dp" />

                        </TableRow>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
        </TableRow>

    </LinearLayout>
</LinearLayout>